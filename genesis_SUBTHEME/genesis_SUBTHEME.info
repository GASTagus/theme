  ; $Id$


  ; Change the name to match your new subthemes name. Modify the optional description.
		
name         = Genesis SUBTHEME
description  = My Kickn' Sub-theme
core         = 7.x
engine       = phptemplate
screenshot   = screenshot_subtheme.png
base theme   = genesis

  ; Genesis subtheme modular-stylesheets.

  ; Options Dark style theme
;stylesheets[all][] = css/dark/dark.css

  ; Uncomment if using Gpanels. There are two: 960 Grids based and Fluid.
;stylesheets[all][] = css/gpanels/gpanels-960-fixed.css
;stylesheets[all][] = css/gpanels/gpanels-fluid.css

  ; HTML elements.
stylesheets[all][] = css/html-elements/type.css
stylesheets[all][] = css/html-elements/lists.css
stylesheets[all][] = css/html-elements/forms.css
stylesheets[all][] = css/html-elements/tables.css

  ; Node, block, comment and page elements.
stylesheets[all][] = css/fields.css
stylesheets[all][] = css/views.css
stylesheets[all][] = css/nodes.css
stylesheets[all][] = css/blocks.css
stylesheets[all][] = css/comments.css
stylesheets[all][] = css/page.css
stylesheets[all][] = css/style.css

